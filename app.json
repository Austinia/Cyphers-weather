{
  "expo": {
    "name": "사이퍼즈 서버 날씨 앱",
    "slug": "Cypher-weather",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/C.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
